// Dynamic location config - will be populated from dataset
let locationConfig={};
let isLocationConfigLoaded=false;

// Load location configuration from Historical Regions dataset (with pagination)
async function loadLocationConfig(){
    try{
        const datasetId='cmljrq0xj0pp11mpcywksh76c';
        const username='jcbunch3';
        const limit=100;
        let allFeatures=[];
        let startId=null;
        
        while(true){
            let url=`https://api.mapbox.com/datasets/v1/${username}/${datasetId}/features?limit=${limit}&access_token=${MAPBOX_TOKEN}`;
            if(startId){url+=`&start=${startId}`;}
            
            const response=await fetch(url);
            const data=await response.json();
            
            if(!data.features||data.features.length===0) break;
            allFeatures=allFeatures.concat(data.features);
            
            if(data.features.length<limit) break;
            startId=data.features[data.features.length-1].id;
        }
        
        allFeatures.forEach(feature=>{
            const props=feature.properties;
            
            if(props.url&&props.coordinates){
                const urlSlug=props.url.split('/').pop();
                
                if(urlSlug){
                    const coords=typeof props.coordinates==='string'?JSON.parse(props.coordinates):props.coordinates;
                    
                    if(coords&&coords.longitude!=null&&coords.latitude!=null&&coords.zoom!=null){
                        locationConfig[urlSlug]={
                            center:[coords.longitude,coords.latitude],
                            zoom:coords.zoom
                        };
                    }
                }
            }
        });
        isLocationConfigLoaded=true;
        console.log('Location config loaded:',Object.keys(locationConfig).length,'regions from dataset');
    }catch(error){
        console.error('Failed to load location config:',error);
        isLocationConfigLoaded=true;
    }
}
